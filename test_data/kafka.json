      "type": "number"
    },
    "current_cc_errors_global": {
      "type": "integer"
    },
    "disk_stats": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "available_space": {
            "type": "integer"
          },
          "is_removable": {
            "type": "boolean"
