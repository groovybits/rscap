          "capture_iat_max": {
            "type": "integer"
          },
          "capture_time": {
            "type": "integer"
          },
          "continuity_counter": {
            "type": "integer"
          },
          "count": {
            "type": "integer"
          },
          "current_error_count": {
            "type": "integer"
