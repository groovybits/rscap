  "patternProperties": {
    "^[a-zA-Z0-9_-]+\\.(network)$": {
      "type": "object",
      "properties": {
        "errors_on_received": {
          "type": "integer"
        },
        "errors_on_transmitted": {
          "type": "integer"
        },
        "packets_received": {
          "type": "integer"
        },
        "packets_transmitted": {
          "type": "integer"
        },
        "received": {
          "type": "integer"
        },
        "transmitted": {
          "type": "integer"
        }
      },
      "required": [
        "errors_on_received",
        "errors_on_transmitted",
        "packets_received",
        "packets_transmitted",
        "received",
        "transmitted"
      ]
    }
