  "required": [
    "bitrate_avg_global",
    "boot_time",
    "cc_errors_global",
    "core_count",
    "cpu_count",
    "cpu_usage",
    "current_cc_errors_global",
    "disk_stats",
    "host_name",
    "iat_avg_global",
    "iat_max_global",
    "id",
    "kernel_version",
    "load_avg_fifteen",
    "load_avg_five",
    "load_avg_one",
    "os_version",
    "process_count",
    "processes",
    "source_ip",
    "source_port",
    "streams",
    "system_name",
    "timestamp",
    "total_memory",
    "total_swap",
    "uptime",
    "used_memory",
    "used_swap"
  ]
