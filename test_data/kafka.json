          "total_space": {
            "type": "integer"
          }
        },
        "required": ["available_space", "is_removable", "name", "total_space"]
      }
    },
    "host_name": {
      "type": "string"
    },
    "iat_avg_global": {
      "type": "number"
    },
    "iat_max_global": {
      "type": "number"
    },
    "id": {
      "type": "string"
    },
    "image_pts": {
      "type": "integer"
    },
    "kernel_version": {
      "type": "string"
    },
    "load_avg_fifteen": {
      "type": "number"
    },
    "load_avg_five": {
      "type": "number"
    },
    "load_avg_one": {
      "type": "number"
    },
    "log_message": {
      "type": "string"
    },
    "os_version": {
      "type": "string"
    },
    "pid_map": {
      "type": "string"
    },
    "process_count": {
      "type": "integer"
    },
    "processes": {
      "type": "array",
      "items": {
        "type": "string"
      }
