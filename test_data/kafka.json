          "error_count": {
            "type": "integer"
          },
          "iat": {
            "type": "integer"
          },
          "iat_avg": {
            "type": "integer"
          },
          "iat_max": {
            "type": "integer"
          },
          "iat_min": {
            "type": "integer"
          },
          "id": {
            "type": "string"
          },
          "last_arrival_time": {
            "type": "integer"
          },
          "last_sample_time": {
            "type": "integer"
          },
          "packet_count": {
            "type": "integer"
          },
          "packet_len": {
            "type": "integer"
          },
          "packet_start": {
            "type": "integer"
